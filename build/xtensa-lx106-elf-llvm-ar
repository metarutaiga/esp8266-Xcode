#!/bin/bash

ARGS=""
while [[ $# > 0 ]] ; do
    case $1 in
        -arch_only*)
        shift ;;
        -D*)
        ;;
        -o*)
        ;;
        -L*)
        ;;
        -static*)
        ;;
        -syslibroot*)
        shift ;;
        -filelist*)
        while read -r line; do
            ARGS=$line" "$ARGS
        done < $2
        shift ;;
        *)
        ARGS=$1" "$ARGS
        ;;
    esac
    shift
done

echo llvm-ar $ARGS
if [ -f "esp-clang/bin/llvm-ar" ]; then
  esp-clang/bin/llvm-ar r $ARGS
else
  llvm-ar $ARGS
fi